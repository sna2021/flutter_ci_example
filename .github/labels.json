{
  "labels": {
    "ios": {
      "name": "ios",
      "colour": "#000000",
      "description": "IOS folder changed"
    },
    "android": {
      "name": "android",
      "colour": "#32CD32",
      "description": "Android folder changed"
    },
    "web": {
      "name": "web",
      "colour": "#FF00FF",
      "description": "Web folder changed"
    },
    "fix": {
      "name": "fix",
      "colour": "#F4A460",
      "description": "FIX"
    },
    "feature": {
      "name": "feature",
      "colour": "#4682B4",
      "description": "FEATURE"
    },
    "main_error": {
      "name": "MAIN CHANGED!!!",
      "colour": "#CC0000",
      "description": "target has changes"
    }
  },
  "pr": {
    "ios": {
      "requires": 1,
      "conditions": [
        {
          "type": "filesMatch",
          "glob": "ios/**/*"
        },
        {
          "type": "filesMatch",
          "glob": "ios/*"
        }
      ]
    },
    "android": {
      "requires": 1,
      "conditions": [
        {
          "type": "filesMatch",
          "glob": "android/**/*"
        },
        {
          "type": "filesMatch",
          "glob": "android/*"
        }
      ]
    },
    "web": {
      "requires": 1,
      "conditions": [
        {
          "type": "filesMatch",
          "glob": "web/**/*"
        },
        {
          "type": "filesMatch",
          "glob": "web/*"
        }
      ]
    },
    "feature": {
      "requires": 1,
      "conditions": [
        {
          "type": "branchMatches",
          "pattern": "^features\\/"
        },
        {
          "type": "branchMatches",
          "pattern": "^feature\\/"
        }
      ]
    },
    "fix": {
      "requires": 1,
      "conditions": [
        {
          "type": "branchMatches",
          "pattern": "^fix\\/"
        },
        {
          "type": "branchMatches",
          "pattern": "^hotfix\\/"
        }
      ]
    },
    "main_error": {
      "requires": 1,
      "conditions": [
        {
          "type": "filesMatch",
          "glob": "**/lib/main.dart"
        },
        {
          "type": "filesMatch",
          "glob": "**/lib/targets/stage_main.dart"
        },
        {
          "type": "filesMatch",
          "glob": "**/lib/targets/prod_main.dart"
        }
      ]
    }
  }
}